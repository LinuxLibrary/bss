#!/bin/bash
# Author: Arjun Shrinivas
# Date: 24-Jan-2015

################
## Main Code  ##
################

for countries in USA Australia France Latvia Argentina Jamica Cuba
do
  case $countries in USA )
  echo "Welcome to North America"
  Australia )
  echo " Good day mate "
  France )
  echo " Merci "
  Latvia )
  echo " Welcome to the former USSR "
  Argentina )
  echo "Berenous Dias"
  Jamica )
  echo " One Love "
  * )
  echo "welcome to another land"
  esac
done

# End of File
